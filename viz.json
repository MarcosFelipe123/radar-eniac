{
  "data": [
    {
      "id": "concepts",
      "label": "Dimensões (Eixos)",
      "type": "dimension",
      "options": { "min": 1, "max": 1 }
    },
    {
      "id": "metrics",
      "label": "Métricas",
      "type": "metric",
      "options": { "min": 1, "max": 10 }
    }
  ],
  "style": [
    {
      "id": "lineStyle",
      "label": "Estilo das Linhas",
      "elements": [
        {
          "type": "slider",
          "id": "opacity",
          "label": "Opacidade das Áreas",
          "defaultValue": 0.35,
          "min": 0, "max": 1, "step": 0.05
        },
        {
          "type": "slider",
          "id": "strokeWidth",
          "label": "Espessura da Linha",
          "defaultValue": 2,
          "min": 1, "max": 10
        }
      ]
    },
    {
      "id": "gridStyle",
      "label": "Grid e Níveis",
      "elements": [
        {
          "type": "fontColor",
          "id": "gridColor",
          "label": "Cor do Grid",
          "defaultValue": "#999999"
        },
        {
          "type": "numericInput",
          "id": "levels",
          "label": "Níveis do Grid",
          "defaultValue": 5
        },
        {
          "type": "checkbox",
          "id": "showDots",
          "label": "Mostrar Pontos (Marcadores)",
          "defaultValue": true
        }
      ]
    }
  ]
}